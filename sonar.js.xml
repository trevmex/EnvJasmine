<project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>net.project.my</groupId>
  <artifactId>myproject</artifactId>
  <version>1.0.0.0</version>
  <name>My Project</name>
  <description>
    Example Project for EnvJasmine, JSCover, Sonar integration
  </description>
  <build>
    <finalName>my-project</finalName>
    <sourceDirectory>samples</sourceDirectory>
  </build>
  <properties>
    <sonar.language>js</sonar.language>
    <sonar.branch>javascript</sonar.branch>
    <sonar.exclusions>ajaxDemo.js</sonar.exclusions>
    <sonar.javascript.jstestdriver.reportsPath>reports</sonar.javascript.jstestdriver.reportsPath>
    <sonar.javascript.lcov.reportPath>reports/jsTestDriver.conf-coverage.dat</sonar.javascript.lcov.reportPath>
    <sonar.dynamicAnalysis>reuseReports</sonar.dynamicAnalysis>

  </properties>
</project>